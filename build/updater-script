ui_print(" ");
ui_print("Installing ExtremeKernel...");
ui_print(" ");

ui_print("Installing boot image...");
package_extract_file("files/boot.img", "/dev/block/by-name/boot");
ui_print("Full Patching vendor_boot image...");
package_extract_file("files/vendor_boot.img", "/dev/block/by-name/vendor_boot");
ui_print("Full Patching dtbo image...");
package_extract_file("files/dtbo.img", "/dev/block/by-name/dtbo");

ui_print(" ");
ui_print("Warning! If you are coming from stock kernel, please format your data partition, as otherwise your data might get locked");
ui_print("Installation done!");
ui_print("Enjoy!");
ui_print(" ");